<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <!--
        ***************************************************************************
        Green Accents
        ***************************************************************************
    -->

    <LinearGradientBrush x:Key="InputSelectionBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FF98B913" />
        <GradientStop Offset="1" Color="#FF75A20A" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="GradientBackground" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Offset="0" Color="#FFf4f5f6" />
                <GradientStop Offset="1" Color="#FFd0d6db" />
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    
    <LinearGradientBrush x:Key="ColorAccentBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#BCD742" />
        <GradientStop Offset="1" Color="#91B006" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ColorAccentOverBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="1" Color="#c3e040" />
        <GradientStop Color="#9ebf0a" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ColorAccentSubduedBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0.31" Color="#FF6D9000" />
        <GradientStop Offset="0.78" Color="#FF5A8100" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ColorAccentHighlightBrush" Color="#c2df00" />
    <SolidColorBrush x:Key="ColorAccentSolidBrush" Color="#FF75A20A" />

