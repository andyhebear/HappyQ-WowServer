<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<G.O.S.E.World Author="H.Q.Cai" Version="0.1">
  <Settings>

    <ZoneWorld>
      <!--此Zone服务端(在Zone群组中)的唯一描述符-->
      <ZoneID>Zone[Zone名字-World的唯一描述符]</ZoneID>
      <!--此Zone服务端监听的地址-->
      <ZoneHost>127.0.0.1</ZoneHost>
      <!--此Zone服务端监听的端口-->
      <ZonePort>19780</ZonePort>
    </ZoneWorld>

    <ZoneClusterWorld>
      <!--连接至(Zone群组)服务端在DomainWorld中)的(唯一描述符-->
      <ZoneClusterID>ZoneCluster[ZoneCluster名字-World的唯一描述符]</ZoneClusterID>
      <!--连接至(Zone群组)服务端的地址-->
      <ZoneClusterHost>127.0.0.1</ZoneClusterHost>
      <!--连接至(Zone群组)服务端的端口-->
      <ZoneClusterPort>19781</ZoneClusterPort>
    </ZoneClusterWorld>

  </Settings>
</G.O.S.E.World>