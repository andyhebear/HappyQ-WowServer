<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Mmose.World Author="H.Q.Cai" Version="0.0.1.0">
  <Settings>

    <ZoneClusterWorld>
      <!--此ZoneCluste服务端在(ZoneCluster群组)的(唯一描述符-->
      <ZoneClusterID>ZoneCluster[ZoneCluster名字-World的唯一描述符]</ZoneClusterID>
      <!--此ZoneCluste服务端的地址-->
      <ZoneClusterHost>127.0.0.1</ZoneClusterHost>
      <!--此ZoneCluste服务端的端口-->
      <ZoneClusterPort>20001</ZoneClusterPort>
      <!--此ZoneCluster服务端的ZoneCluster名字-->
      <ZoneClusterName>ZoneCluster[Domain名字-ZoneCluster名字]</ZoneClusterName>
    </ZoneClusterWorld>

    <DomainWorld>
      <!--连接至(DomainWorld)服务端(在DomainWorld群组中)的唯一描述符-->
      <DomainID>Domain[Domain名字-World的唯一描述符]</DomainID>
      <!--连接至(DomainWorld)服务端的地址-->
      <DomainHost>127.0.0.1</DomainHost>
      <!--连接至(DomainWorld)服务端的端口-->
      <DomainPort>30000</DomainPort>
    </DomainWorld>

  </Settings>
</Mmose.World>