<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Mmose.WorldOfWarcraft Author="H.Q.Cai" Version="0.0.1.0">
  <Settings>

    <ServerSetting>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的RealmList地址-->
      <RealmServerHost>127.0.0.1</RealmServerHost>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的RealmList端口-->
      <RealmServerPort>8093</RealmServerPort>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的AuthServer地址-->
      <AuthServerHost>127.0.0.1</AuthServerHost>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的AuthServer端口-->
      <AuthServerPort>3724</AuthServerPort>
    </ServerSetting>

    <GameSetting>
    </GameSetting>

    <SQLSetting>
      <!--SQL登陆的用户名-->
      <SQLUser>sa</SQLUser>
      <!--SQL的主机-->
      <SQLHost>localhost</SQLHost>
      <!--SQL的端口-->
      <SQLPort>1433</SQLPort>
      <!--SQL的数据库名-->
      <SQLDatabase>WorldOfWarcraft</SQLDatabase>
      <!--连接SQL数据库的类型-->
      <SQLConnection>mssql2005</SQLConnection>
    </SQLSetting>

  </Settings>
</Mmose.WorldOfWarcraft>