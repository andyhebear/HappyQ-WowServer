<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Mmose.WorldOfWarcraft Author="H.Q.Cai" Version="0.0.1.0">
  <Settings>

    <ServerSetting>
      <!--此ZoneCluster服务端连接的WorldOfWarcraft游戏服务的RealmList地址-->
      <RealmServerHost>127.0.0.1</RealmServerHost>
      <!--此ZoneCluster服务端连接的WorldOfWarcraft游戏服务的RealmList端口-->
      <RealmServerPort>8093</RealmServerPort>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的WorldServer地址-->
      <WorldServerHost>127.0.0.1</WorldServerHost>
      <!--此ZoneCluster服务端监听的WorldOfWarcraft游戏服务的WorldServer端口-->
      <WorldServerPort>8129</WorldServerPort>
    </ServerSetting>

    <WorldSetting>
      <!--此WorldOfWarcraft服务的游戏世界的名称-->
      <WorldName>WorldName名字</WorldName>
      <!--此WorldOfWarcraft服务的游戏世界的名称颜色(暂时不要改)-->
      <Colour>1</Colour>
      <!--此WorldOfWarcraft服务的游戏世界的区域(中国,台湾...)位置(暂时不要改)-->
      <TimeZone>1</TimeZone>
      <!--此WorldOfWarcraft服务的游戏世界的人口数(暂时不要改)-->
      <Population>0</Population>
      <!--此WorldOfWarcraft服务的游戏类型 PVP/RP/RPPVP/NORMAL-->
      <Icon>Normal</Icon>
    </WorldSetting>
    
    <GameSetting>
    </GameSetting>

    <SQLSetting>
      <!--SQL登陆的用户名-->
      <SQLUser>sa</SQLUser>
      <!--SQL的主机-->
      <SQLHost>localhost</SQLHost>
      <!--SQL的端口-->
      <SQLPort>1433</SQLPort>
      <!--SQL的数据库名-->
      <SQLDatabase>WorldOfWarcraft</SQLDatabase>
      <!--连接SQL数据库的类型-->
      <SQLConnection>mssql2005</SQLConnection>
    </SQLSetting>

  </Settings>
</Mmose.WorldOfWarcraft>